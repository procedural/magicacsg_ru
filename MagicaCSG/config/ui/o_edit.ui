		{
			hbox::
			{
				option(def = 'op-blue' id = '0'	textFont = 'std' text = 'X1'  textAlign = '2' textSize = '6.5 6.5')(flex = '0.8')(hint = 'Ограничение масштабирования по одну сторону оси [1]')
